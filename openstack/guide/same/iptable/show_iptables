#!/bin/sh
echo "========================================================"
echo "filter"
echo "========================================================"
iptables -t filter -S
echo "========================================================"
echo "nat"
echo "========================================================"
iptables -t nat -S
echo "========================================================"
echo "mangle"
echo "========================================================"
iptables -t mangle -S
echo "========================================================"
echo "raw"
echo "========================================================"
iptables -t raw -S
